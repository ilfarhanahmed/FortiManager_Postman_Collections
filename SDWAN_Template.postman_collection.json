    {
      "name": "GET - Template Data",
      "event": [
        {
          "listen": "test",
          "script": {
            "exec": [
              "const response = pm.response.json();\r",
              "const sdwan = response.result[0].data;\r",
              "\r",
              "// SD-WAN Members\r",
              "const members = sdwan.members.map(m => ({\r",
              "    interface: m.interface[0],\r",
              "    priority: m.priority,\r",
              "    weight: m.weight,\r",
              "    status: m.status,\r",
              "    zone: m.zone.join(\", \")\r",
              "}));\r",
              "\r",
              "// Health Checks\r",
              "const healthChecks = sdwan[\"health-check\"].map(h => ({\r",
              "    name: h.name,\r",
              "    server: h.server?.join(\", \") || \"N/A\",\r",
              "    protocol: h.protocol || \"N/A\",\r",
              "    latencyThreshold: h.sla?.[0]?.[\"latency-threshold\"] ?? \"N/A\",\r",
              "    jitterThreshold: h.sla?.[0]?.[\"jitter-threshold\"] ?? \"N/A\",\r",
              "    packetlossThreshold: h.sla?.[0]?.[\"packetloss-threshold\"] ?? \"N/A\"\r",
              "}));\r",
              "\r",
              "// Services\r",
              "const services = sdwan.service.map(s => ({\r",
              "    name: s.name,\r",
              "    dst: s.dst.join(\", \"),\r",
              "    src: s.src.join(\", \"),\r",
              "    loadBalance: s[\"load-balance\"],\r",
              "    shortcut: s.shortcut\r",
              "}));\r",
              "\r",
              "// Neighbors\r",
              "const neighbors = sdwan.neighbor.map(n => ({\r",
              "    ip: n.ip?.join(\", \") || \"N/A\",\r",
              "    mode: n.mode,\r",
              "    role: n.role,\r",
              "    member: n.member?.join(\", \"),\r",
              "    slaId: n[\"sla-id\"] || \"N/A\",\r",
              "    healthCheck: n[\"health-check\"]?.join(\", \") || \"N/A\"\r",
              "}));\r",
              "\r",
              "// Duplication rules\r",
              "const duplications = sdwan.duplication.map(d => ({\r",
              "    id: d.id,\r",
              "    srcIntf: d.srcintf.join(\", \"),\r",
              "    dstIntf: d.dstintf.join(\", \"),\r",
              "    srcAddr: d.srcaddr.join(\", \"),\r",
              "    dstAddr: d.dstaddr.join(\", \"),\r",
              "    service: d.service.join(\", \"),\r",
              "    packetDuplication: d[\"packet-duplication\"],\r",
              "    packetDeDuplication: d[\"packet-de-duplication\"]\r",
              "}));\r",
              "\r",
              "// Visualizer output\r",
              "pm.visualizer.set(`\r",
              "<style>\r",
              "    table { border-collapse: collapse; width: 100%; margin-bottom: 20px; }\r",
              "    th, td { border: 1px solid #ccc; padding: 6px; text-align: left; }\r",
              "    th { background-color: #f2f2f2; }\r",
              "    h2 { margin-top: 30px; }\r",
              "</style>\r",
              "\r",
              "<h2>SD-WAN Members</h2>\r",
              "<table>\r",
              "  <tr><th>Interface</th><th>Priority</th><th>Weight</th><th>Status</th><th>Zone</th></tr>\r",
              "  {{#each members}}\r",
              "    <tr>\r",
              "      <td>{{interface}}</td>\r",
              "      <td>{{priority}}</td>\r",
              "      <td>{{weight}}</td>\r",
              "      <td>{{status}}</td>\r",
              "      <td>{{zone}}</td>\r",
              "    </tr>\r",
              "  {{/each}}\r",
              "</table>\r",
              "\r",
              "<h2>Health Checks</h2>\r",
              "<table>\r",
              "  <tr><th>Name</th><th>Server</th><th>Protocol</th><th>Latency</th><th>Jitter</th><th>Packet Loss</th></tr>\r",
              "  {{#each healthChecks}}\r",
              "    <tr>\r",
              "      <td>{{name}}</td>\r",
              "      <td>{{server}}</td>\r",
              "      <td>{{protocol}}</td>\r",
              "      <td>{{latencyThreshold}}</td>\r",
              "      <td>{{jitterThreshold}}</td>\r",
              "      <td>{{packetlossThreshold}}</td>\r",
              "    </tr>\r",
              "  {{/each}}\r",
              "</table>\r",
              "\r",
              "<h2>SD-WAN Services</h2>\r",
              "<table>\r",
              "  <tr><th>Name</th><th>Destination</th><th>Source</th><th>Load Balance</th><th>Shortcut</th></tr>\r",
              "  {{#each services}}\r",
              "    <tr>\r",
              "      <td>{{name}}</td>\r",
              "      <td>{{dst}}</td>\r",
              "      <td>{{src}}</td>\r",
              "      <td>{{loadBalance}}</td>\r",
              "      <td>{{shortcut}}</td>\r",
              "    </tr>\r",
              "  {{/each}}\r",
              "</table>\r",
              "\r",
              "<h2>SD-WAN Neighbors</h2>\r",
              "<table>\r",
              "  <tr><th>IP</th><th>Mode</th><th>Role</th><th>Member</th><th>SLA ID</th><th>Health Check</th></tr>\r",
              "  {{#each neighbors}}\r",
              "    <tr>\r",
              "      <td>{{ip}}</td>\r",
              "      <td>{{mode}}</td>\r",
              "      <td>{{role}}</td>\r",
              "      <td>{{member}}</td>\r",
              "      <td>{{slaId}}</td>\r",
              "      <td>{{healthCheck}}</td>\r",
              "    </tr>\r",
              "  {{/each}}\r",
              "</table>\r",
              "\r",
              "<h2>Duplication Rules</h2>\r",
              "<table>\r",
              "  <tr><th>ID</th><th>Source Interface</th><th>Destination Interface</th><th>Source Address</th><th>Destination Address</th><th>Service</th><th>Duplication</th><th>De-Duplication</th></tr>\r",
              "  {{#each duplications}}\r",
              "    <tr>\r",
              "      <td>{{id}}</td>\r",
              "      <td>{{srcIntf}}</td>\r",
              "      <td>{{dstIntf}}</td>\r",
              "      <td>{{srcAddr}}</td>\r",
              "      <td>{{dstAddr}}</td>\r",
              "      <td>{{service}}</td>\r",
              "      <td>{{packetDuplication}}</td>\r",
              "      <td>{{packetDeDuplication}}</td>\r",
              "    </tr>\r",
              "  {{/each}}\r",
              "</table>\r",
              "`, {\r",
              "    members,\r",
              "    healthChecks,\r",
              "    services,\r",
              "    neighbors,\r",
              "    duplications\r",
              "});\r",
              ""
            ],
            "type": "text/javascript",
            "packages": {}
          }
        }
      ],
      "request": {
        "method": "POST",
        "header": [],
        "body": {
          "mode": "raw",
          "raw": "{\r\n  \"method\": \"get\",\r\n  \"params\": [\r\n    {\r\n      \r\n     \"url\": \"/pm/config/adom/{{ADOM}}/wanprof/{{TEMPLATE_NAME}}/system/sdwan\"\r\n    }\r\n  ],\r\n  \"session\": \"{{session}}\",\r\n  \"id\": 1,\r\n  \"verbose\": 1\r\n}",
          "options": {
